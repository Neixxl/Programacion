Algoritmo propconmutativanei
	
	Definir a,b Como Logico;
	
	Escribir "Escribe el valor de A";
	Leer a;
	Escribir "Escribe el valor de B";
	Leer b;
	
	Escribir "A = " a, ", B = ", b;
	Escribir "Conmutativa AND: A y B = ", a y b, ", B y A: ", b y a;
	Escribir "Conmutativa OR: A y B = ", a o b, ", B y A: ", b o a;
	
	
	
FinAlgoritmo
